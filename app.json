{
  "expo": {
    "name": "MahjongLah",
    "slug": "MahjongLah",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MahjongLah",
      "cocoaPods": {
        "extraPods": [
          {
            "podName": "FirebaseAuth",
            "modularHeaders": true
          },
          {
            "podName": "FirebaseCore",
            "modularHeaders": true
          },
          {
            "podName": "FirebaseCoreInternal",
            "modularHeaders": true
          },
          {
            "podName": "GoogleUtilities",
            "modularHeaders": true
          },
          {
            "podName": "FirebaseAppCheckInterop",
            "modularHeaders": true
          },
          {
            "podName": "RecaptchaInterop",
            "modularHeaders": true
          },
          {
            "podName": "FirebaseAuthInterop",
            "modularHeaders": true
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.MahjongLah"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "84f1ca6f-64ab-412f-8b68-ac9ce8ab6b1b"
      }
    },
    "owner": "ztinaxz"
  }
}
